#!/usr/bin/sh

[ "$(stat -c %y ~/.cache/corona 2>/dev/null | cut -d' ' -f1)" != "$(date '+%Y-%m-%d')" ] &&
	curl -s https://corona-stats.online/us > ~/.cache/corona

grep "Ohio" ~/.cache/corona |
	sed "s/\s*//g ; s/â•‘//g ; s/â”‚/;/g" |
	awk -F';' '{print "$ðŸ˜·" $3 " (" $9") ðŸ’€ " $5 " ("$7"%)"}'



